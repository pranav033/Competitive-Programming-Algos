// CPP code to illustrate
// the use of sqrt function.
#include <cmath>
#include <iomanip>
#include <iostream>
  
using namespace std;
  
int main()
{
    double val1 = 225.0;
    double val2 = 300.0;
  
    cout << fixed << setprecision(12) << sqrt(val1) << endl;
    cout << fixed << setprecision(12) << sqrt(val2) << endl;
  
    return (0);
}
